<query id="insertLog" action="insert">
    <tables>
        <table name="iconshop_log" />
    </tables>
    <columns>
        <column name="data_srl" var="data_srl" filter="number" notnull="notnull" />
        <column name="icon_srl" var="icon_srl" filter="number" notnull="notnull" />
        <column name="sender_srl" var="sender_srl" filter="number" default="0" />
        <column name="receive_srl" var="receive_srl" filter="number" notnull="notnull" />
        <column name="category_srl" var="category_srl" filter="number" notnull="notnull" />
        <column name="point" var="point" filter="number" default="0" />
        <column name="content" var="content" notnull="notnull" />
        <column name="regdate" default="curdate()" />
        <column name="ipaddress" default="ipaddress()" />
    </columns>
</query>
