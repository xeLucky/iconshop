<query id="getMemberIconCount" action="select">
    <tables>
        <table name="iconshop_user_item" />
    </tables>
    <columns>
        <column name="count(*)" alias="count" />
    </columns>
    <conditions>
        <condition operation="equal" column="member_srl" var="member_srl" filter="number" notnull="notnull" />
    </conditions>
</query>
